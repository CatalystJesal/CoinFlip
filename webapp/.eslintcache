s.recentWinners);\r\n\r\n  const dispatch = useDispatch();\r\n\r\n  useEffect(() => {\r\n    getRecentWinners();\r\n    console.log(\"Recent Winners useEffect()\");\r\n  }, [isRecentWinners]);\r\n\r\n  const getRecentWinners = () => {\r\n    contractInstance.getPastEvents(\r\n      \"FlipOutcome\",\r\n      {\r\n        fromBlock: 0,\r\n        toBlock: \"latest\",\r\n      },\r\n      function (error, event) {\r\n        if (!error) {\r\n          transformData(event, recordLimit);\r\n        } else {\r\n          dispatch(fetch_RecentWinnersFailure(error.message));\r\n        }\r\n      }\r\n    );\r\n  };\r\n\r\n  const transformData = (eventData, recordLimit) => {\r\n    var records = [];\r\n    eventData.reverse().map((data) => {\r\n      if (data.returnValues.won > 0) {\r\n        const winner = {\r\n          blockNumber: data.blockNumber,\r\n          timestamp: 0,\r\n          address: data.returnValues.player,\r\n          won: web3.utils.fromWei(data.returnValues.won, \"ether\") + \" ETH\",\r\n        };\r\n\r\n        records.push(winner);\r\n      }\r\n    });\r\n\r\n    dispatch(fetch_RecentWinnersSuccess(records.slice(0, recordLimit)));\r\n  };\r\n\r\n  const showTableRow = () => {\r\n    return recentWinners.map(({ blockNumber, address, won }) => (\r\n      <tr className=\"table-active\" key={blockNumber}>\r\n        <td>{address}</td>\r\n        <td>{won}</td>\r\n      </tr>\r\n    ));\r\n  };\r\n\r\n  return (\r\n    <div className=\"box1\">\r\n      <table className=\"table\">\r\n        <tbody>\r\n          <tr className=\"tableHeader\">\r\n            <th>Recent wins:</th>\r\n          </tr>\r\n          <tr className=\"table-active\">\r\n            <th scope=\"col\">Address</th>\r\n            <th scope=\"col\">Won</th>\r\n          </tr>\r\n          {isRecentWinners ? (\r\n            showTableRow()\r\n          ) : (\r\n            <tr>\r\n              <td>loading...</td>\r\n            </tr>\r\n          )}\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default RecentWinners;\r\n","D:\\Ivan On Tech Academy\\Ethereum Programming 201\\CoinFlip\\webapp\\src\\redux\\player\\playerTypes.js",[],"D:\\Ivan On Tech Academy\\Ethereum Programming 201\\CoinFlip\\webapp\\src\\components\\game\\Form.js",[],"D:\\Ivan On Tech Academy\\Ethereum Programming 201\\CoinFlip\\webapp\\src\\redux\\baseProps\\basePropsReducer.js",[],"D:\\Ivan On Tech Academy\\Ethereum Programming 201\\CoinFlip\\webapp\\src\\redux\\player\\playerReducer.js",[],{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","severity":1,"message":"108","line":44,"column":6,"nodeType":"109","endLine":44,"endColumn":8,"suggestions":"110"},{"ruleId":"107","severity":1,"message":"111","line":76,"column":6,"nodeType":"109","endLine":76,"endColumn":8,"suggestions":"112"},{"ruleId":"113","severity":1,"message":"114","line":2,"column":23,"nodeType":"115","messageId":"116","endLine":2,"endColumn":34},{"ruleId":"117","severity":1,"message":"118","line":38,"column":26,"nodeType":"119","messageId":"120","endLine":38,"endColumn":28},{"ruleId":"107","severity":1,"message":"121","line":40,"column":21,"nodeType":"122","endLine":40,"endColumn":39},{"ruleId":"107","severity":1,"message":"123","line":41,"column":19,"nodeType":"122","endLine":41,"endColumn":35},{"ruleId":"107","severity":1,"message":"121","line":56,"column":23,"nodeType":"122","endLine":56,"endColumn":48},{"ruleId":"113","severity":1,"message":"124","line":71,"column":9,"nodeType":"115","messageId":"116","endLine":71,"endColumn":18},{"ruleId":"107","severity":1,"message":"123","line":71,"column":21,"nodeType":"125","endLine":71,"endColumn":22},{"ruleId":"107","severity":1,"message":"126","line":26,"column":6,"nodeType":"109","endLine":26,"endColumn":24,"suggestions":"127"},{"ruleId":"113","severity":1,"message":"128","line":1,"column":17,"nodeType":"115","messageId":"116","endLine":1,"endColumn":25},{"ruleId":"113","severity":1,"message":"1